   "id": "301a5f20-063e-49a7-aac6-d091da177c1b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def tide_height(time, amplitude_1, period_1, amplitude_2, period_2):\n",
    "\n",
    "    tide_height = amplitude_1 * np.sin(2 * np.pi * time / period_1) + \\\n",
    "                amplitude_2 * np.sin(2 * np.pi * time / period_2)\n",
    "    \n",
    "    return tide_height"
   ]
